<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AlcoholAddictionVideosPage"
    android:orientation="vertical"
    android:background="@color/app_pages_bg"
    android:id="@+id/my_drawer_layout">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/alcohol_addiction_videos_page_relative_layout">

        </RelativeLayout>
    </ScrollView>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <include layout="@layout/page_toolbar"
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
        <include layout="@layout/bottom_nav_view"
            android:id="@+id/bottom_nav_id"
            />
    </RelativeLayout>
    <include
        layout="@layout/nav_view"
        android:visibility="invisible"/>

</androidx.drawerlayout.widget.DrawerLayout>